#!/bin/bash
set -eux

. "$(dirname "$0")/../vars"

instance=$(aws ec2 run-instances \
                   --subnet-id subnet-086c394e \
                   --image-id "$ami" \
                   --instance-type g2.2xlarge \
                   --security-group-ids '["sg-15464c77"]' \
                   --key-name nelhage-1 \
                   --output=text \
                   --query='Instances[0].InstanceId')
echo "Launched $instance..."
